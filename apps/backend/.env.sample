# Port on which the backend will listen
PORT=3001

# Supabase/Postgres credentials
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-key

# S3-compatible storage for resumes and prep packs
S3_ENDPOINT=https://s3.your-cloud-provider.com
S3_ACCESS_KEY_ID=your-s3-access-key-id
S3_SECRET_ACCESS_KEY=your-s3-secret
S3_BUCKET=resumes

# AI/ML provider keys
OPENAI_API_KEY=your-openai-api-key
HUGGINGFACE_TOKEN=your-huggingface-token

# Email / calendar providers
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-client-secret

# Webhook secrets for ATS and Outreach integrations
ATS_WEBHOOK_SECRET=your-ats-webhook-secret
OUTREACH_WEBHOOK_SECRET=your-outreach-webhook-secret

# Encryption key for data at rest and JWT secrets
ENCRYPTION_AT_REST_KEY=your-encryption-key
JWT_SECRET=your-jwt-secret
